{
  "name": "@fresha/openapi-diff",
  "version": "0.1.1",
  "description": "Diffing/versioning tool for OpenAPI schemas",
  "main": "build/index.js",
  "bin": {
    "fresha-openapi-diff": "./bin/fresha-openapi-diff.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "check": "run-s lint test",
    "check:fix": "run-s lint:fix test",
    "clean": "rimraf ./build",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "lint": "run-s eslint typecheck",
    "lint:fix": "run-s eslint:fix typecheck",
    "prebuild": "npm run clean",
    "prebuild:watch": "npm run clean",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "build/",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "OpenAPI",
    "diff",
    "versioning"
  ],
  "author": "Fresha Engineering",
  "contributors": [
    {
      "name": "Andriy Mykulyak",
      "email": "andriy@fresha.com",
      "url": "https://github.com/mykulyak"
    }
  ],
  "maintainers": [
    {
      "name": "Andriy Mykulyak",
      "email": "andriy@fresha.com",
      "url": "https://github.com/mykulyak"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@fresha/eslint-config": "^0.1.1",
    "@fresha/jest-config": "^0.1.1",
    "@fresha/typescript-config": "^0.1.1",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@fresha/api-tools-core": "^0.4.2",
    "@fresha/openapi-model": "^0.6.2",
    "chalk": "^4.1.2",
    "yargs": "^17.6.2"
  }
}

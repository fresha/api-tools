{
  "name": "@fresha/openapi-codegen-test-utils",
  "version": "0.1.2",
  "description": "Testing utilites for OpenAPI codegens",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "check": "run-s lint test",
    "check:fix": "run-s lint:fix test",
    "clean": "rimraf ./build",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "lint": "run-s eslint typecheck",
    "lint:fix": "run-s eslint:fix typecheck",
    "prebuild": "npm run clean",
    "prebuild:watch": "npm run clean",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "JSON:API"
  ],
  "author": "Fresha Engineering",
  "contributors": [
    {
      "name": "Andriy Mykulyak",
      "email": "andriy@fresha.com",
      "url": "https://github.com/mykulyak"
    }
  ],
  "maintainers": [
    {
      "name": "Andriy Mykulyak",
      "email": "andriy@fresha.com",
      "url": "https://github.com/mykulyak"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@fresha/eslint-config": "^0.1.1",
    "@fresha/typescript-config": "^0.1.1",
    "@types/jest": "^29.1.2",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@fresha/api-tools-core": "^0.4.2",
    "@fresha/code-morph-test-utils": "^0.1.1",
    "@fresha/openapi-codegen-utils": "^0.1.3",
    "@fresha/openapi-model": "^0.6.0",
    "jest-matcher-utils": "^29.1.2",
    "ts-morph": "^16.0.0"
  }
}
